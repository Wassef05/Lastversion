import { OnChanges, OnDestroy, SimpleChanges, TemplateRef } from '@angular/core';
import { TeleportService } from './teleport.service';
export declare class TeleportDirective implements OnChanges, OnDestroy {
    private tpl;
    private service;
    teleportTo: string | null | undefined;
    private viewRef;
    private subscription;
    constructor(tpl: TemplateRef<any>, service: TeleportService);
    ngOnChanges(changes: SimpleChanges): void;
    ngOnDestroy(): void;
    private dispose;
}
export declare class TeleportModule {
}
